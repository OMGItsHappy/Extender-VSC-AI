{
  "name": "open-ai-integration",
  "displayName": "Open-AI Integration",
  "description": "Use Open-AI's LLMs to help handle error codes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-ai-integration.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "open-ai-integration.showSelection",
        "title": "Show Selection"
      },
      {
        "command": "open-ai-integration.getClipboardText",
        "title": "Get Clipboard Text"
      },
      {
        "command":"open-ai-integration.openAIHelloWorld",
        "title": "Open AI Hello World"
      },
      {
        "command":"open-ai-integration.openCustomPanel",
        "title": "Open Custom Pannel"
      },
      {
        "command":"open-ai-integration.showOutputPanel",
        "title": "Open Bottom Custom Pannel"
      },
      {
        "command":"open-ai-integration.hideOutputPanel",
        "title": "Hide Custom Pannel"
      },
      {
        "command":"open-ai-integration.clearOutputPanel",
        "title": "Clear Custom Pannel"
      },
      {
        "command":"open-ai-integration.showCustomOutput",
        "title": "Show Custom Output"
      },
      {
        "command":"open-ai-integration.openAIExplainInOutputPanel",
        "title":"Open AI Explain"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "open-ai-integration.openAIExplainInOutputPanel",
          "when": "editorFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.2",
    "openai": "^4.24.7"
  }
}
